{
  "name": "hello-plugin",
  "displayName": "hello-plugin",
  "description": "It is just test plugin",
  "version": "0.0.1",
  "icon": "png-icons.png",
  "license": "Apache 2.0",
  "engines": {
    "vscode": "^1.107.0"
  },
  "categories": [
    "Programming Languages",
    "Machine Learning",
    "Snippets"  
  ],
  "keywords": [
    "ai",
    "artificial intelligence",
    "code assistant",
    "code completion",
    "productivity",
    "code helper",
    "refactoring",
    "code suggestions",
    "intelligent coding",
    "developer tools"
  ],
  "bugs": {
    "url": "https://github.com/yourname/code-helper-agent/issues",
    "email": "support@yourname.com"
  },
  "homepage": "https://github.com/vikas-nimje/visual-studio-extension-proj/blob/main/README.md",
  "activationEvents": [
    "onCommand:hello-plugin.hello"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hello-plugin.hello",
        "title": "Hello VS Code Plugin"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.107.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
